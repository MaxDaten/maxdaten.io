{
    "redirects": [
        {
            "source": "/",
            "has": [{ "type": "host", "value": "www.maxdaten.io" }],
            "destination": "/en",
            "statusCode": 302
        },
        {
            "source": "/",
            "has": [{ "type": "host", "value": "maxdaten.io" }],
            "destination": "/en",
            "statusCode": 302
        },
        {
            "source": "/:path*",
            "has": [{ "type": "host", "value": "www.maxdaten.de" }],
            "destination": "https://maxdaten.de/:path*",
            "statusCode": 301
        },
        {
            "source": "/en/:path*",
            "has": [{ "type": "host", "value": "maxdaten.de" }],
            "destination": "https://maxdaten.io/en/:path*",
            "statusCode": 301
        }
    ],
    "rewrites": [
        {
            "source": "/scitylana/js/script.js",
            "destination": "https://plausible.io/js/script.js"
        },
        {
            "source": "/scitylana/api/event",
            "destination": "https://plausible.io/api/event"
        }
    ]
}
